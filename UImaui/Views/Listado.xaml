<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UImaui.Views.Listado"
             xmlns:miViewmodelA="clr-namespace:UImaui.ViewModels"
             Title="Listado">

    <ContentPage.BindingContext>
        <miViewmodelA:PaginasVM></miViewmodelA:PaginasVM>
    </ContentPage.BindingContext>


    <VerticalStackLayout>
        <Button
            Text="Personas"
            Command="{Binding IrAPersonasCommand}"
        />


        <Button
            Text="Departamentos"
            Command="{Binding IrADeptsCommand}"
        />

    </VerticalStackLayout>


</ContentPage>